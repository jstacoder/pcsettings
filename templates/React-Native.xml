    <variable name="styles" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="expcomp" value="export default class $ComponentName$ extends React.Component {&#10;  render(){&#10;    return (&#10;      $JSX$&#10;    )&#10;  }&#10;}" description="export a component" toReformat="false" toShortenFQNames="true">
