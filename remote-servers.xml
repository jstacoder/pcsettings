<application>
  <component name="RemoteServers">
    <remote-server name="Docker" type="docker">
      <configuration>
        <option name="apiUrl" value="https://192.168.99.100:2376" />
        <option name="certificatesPath" value="$USER_HOME$/.docker/machine/machines/default" />
        <option name="machine" value="default" />
        <option name="pathMappings">
          <list>
            <DockerPathMappingImpl>
              <option name="localPath" value="/Users" />
              <option name="virtualMachinePath" value="/Users" />
            </DockerPathMappingImpl>
            <DockerPathMappingImpl>
              <option name="localPath" value="/var/run/docker.sock" />
              <option name="virtualMachinePath" value="/var/run/docker.sock" />
            </DockerPathMappingImpl>
          </list>
        </option>
      </configuration>
    </remote-server>
    <remote-server name="Docker fo Wac" type="docker">
      <configuration>
        <option name="apiUrl" value="unix:///var/run/docker.sock" />
        <option name="certificatesPath" />
      </configuration>
    </remote-server>
  </component>
</application>